        <servlet-class>com.ers.v1.servlet.series.GetFactorsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadSeriesServlet</servlet-name>
        <servlet-class>com.ers.v1.servlet.series.LoadSeriesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadFactorsServlet</servlet-name>
        <servlet-class>com.ers.v1.servlet.esg.LoadFactorsServlet</servlet-class>
