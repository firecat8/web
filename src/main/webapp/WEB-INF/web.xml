    <servlet-mapping>
        <servlet-name>LoadFactorsServlet</servlet-name>
        <url-pattern>/esg/loadFactors</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SuggestFactorsServlet</servlet-name>
        <url-pattern>/esg/suggestFactors</url-pattern>
