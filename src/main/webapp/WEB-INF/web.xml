    <servlet-mapping>
        <servlet-name>DeleteSeriesServlet</servlet-name>
        <url-pattern>/deleteSeries</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadSessionQuotesServlet</servlet-name>
        <url-pattern>/loadQuotes</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>PredictionServlet</servlet-name>
        <servlet-class>com.ers.v1.servlet.prediction.PredictionServlet</servlet-class>
        <load-on-startup>0</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>UploaderServlet</servlet-name>
        <servlet-class>com.ers.v1.servlet.UploaderServlet</servlet-class>
        <multipart-config>
            <max-file-size>10485760</max-file-size>
            <max-request-size>20971520</max-request-size>
            <file-size-threshold>5242880</file-size-threshold>
        </multipart-config>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.ers.v1.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.ers.v1.servlet.LogoutServlet</servlet-class>
