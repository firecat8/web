<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>DeletePredictionResultsServlet</servlet-name>
        <servlet-class>com.ers.v1.servlet.prediction.DeletePredictionResultsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExportEsgResultServlet</servlet-name>
        <servlet-class>com.ers.v1.servlet.excel.ExportEsgResultServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QuotesUploaderServlet</servlet-name>
        <servlet-class>com.ers.v1.servlet.QuotesUploaderServlet</servlet-class>
        <multipart-config>
            <max-file-size>10485760</max-file-size>
            <max-request-size>20971520</max-request-size>
            <file-size-threshold>5242880</file-size-threshold>
        </multipart-config>
    </servlet>
    <servlet>
        <servlet-name>DeleteSeriesServlet</servlet-name>
        <servlet-class>com.ers.v1.servlet.series.DeleteSeriesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadSessionQuotesServlet</servlet-name>
        <servlet-class>com.ers.v1.servlet.series.LoadSessionQuotesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ExportPredictionresultsServlet</servlet-name>
        <url-pattern>/exportPredictResults</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeletePredictionResultsServlet</servlet-name>
        <url-pattern>/deletePredictionResults</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExportEsgResultServlet</servlet-name>
        <url-pattern>/exportEsgResults</url-pattern>
